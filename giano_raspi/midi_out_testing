import mido

print(mido.get_output_names())
print(mido.get_input_names())

with mido.open_output() as out_:
	with mido.open_input() as in_:
		print("out:")
		print(out_)
		print("in:")
		print(in_)

		msg = mido.Message('note_on', note= 60) # test note
		out_.send(msg)
		print("MESSAGE SENT YAY")

		for msg in in_.iter_pending():
			if msg:
				print("O YA MESSAGE RECEIEVED")
				print(msg)
				break 
			else: print("msg not sent sky probably made a typo")
